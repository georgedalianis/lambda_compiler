a, b: integer;

def cube(i: integer)-> integer:
   return i*i*i;
enddef;
   
def add(n: integer, k: integer) -> integer:
   j: integer;
  
   j = (N-n) + cube(k);
   writeInteger(j);
   return j;
enddef;

